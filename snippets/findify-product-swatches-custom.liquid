{% for variant in variants %}
  {% if variant contains 'pr=' %}
    {%- assign variant_price = variant | split: 'pr=' | last | split: '|' | first -%}{% endif %}
  {% if variant contains 'pu=' %}
    {%- assign variant_product_url = variant | split: 'pu=' | last | split: '|' | first -%}{% endif %}
  {% if variant contains 'qu=' %}
    {%- assign variant_quantity = variant | split: 'qu=' | last | split: '|' | first -%}{% endif %}
  {% if variant contains 'ras=' %}
    {%- assign variant_reviews = variant | split: 'ras=' | last | split: '|' | first -%}{% endif %}

  {% if variant contains 'si=' %}
    {%- assign variant_size = variant | split: 'si=' | last | split: '|' | first -%}{% endif %}

  {% if variant contains 'sk=' %}
    {%- assign variant_sku = variant | split: 'sk=' | last | split: '|' | first -%}{% endif %}
  {% if variant contains 'ta=' %}
    {%- assign variant_tags = variant | split: 'ta=' | last | split: '|' | first -%}{% endif %}
  {% if variant contains 'ti=' %}
    {%- assign variant_title = variant | split: 'ti=' | last | split: '|' | first -%}{% endif %}
  {% if variant contains 'ua=' %}
    {%- assign variant_updated_at = variant | split: 'ua=' | last | split: '|' | first -%}{% endif %}
  {% if variant contains 'st=' %}
    {%- assign variant_stickers = variant | split: 'st=' | last | split: '|' | first -%}{% endif %}
  {% if variant contains 'id=' %}
    {%- assign variant_id = variant | split: 'st=' | last | split: '|' | first -%}
  {% endif %}

  <script>
      console.log('Custom swatches. Here you have different fields coming from Findify. But also you can access product directly from Shopify.', "{{ variant_id }}")
  </script>
{% endfor %}